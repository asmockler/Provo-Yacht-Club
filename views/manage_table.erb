<% @song.each do |song| %>


  <tr id="<%= song.id %>">
    <td style="width:auto"><a class="edit btn btn-primary btn-xs btn-block" href="/Manager/edit-modal/<%= song.id %>"> <span class="glyphicon glyphicon-pencil" style="width:40px"></span> </a></td>
    <td style="width:auto"><a class="delete btn btn-danger btn-xs btn-block" href="/Manager/delete/<%= song.id %>"><span class="glyphicon glyphicon-trash" style="width:40px"></span></a></td>
    <td><%= song.title %></td>
    <td class="hidden-sm hidden-xs"><%= song.artist %></td>
    <td class="hidden-sm hidden-xs"><%= song.album %></td>
    <td class="hidden-sm hidden-xs"><%= song.created_at %></td>
    <td class="hidden-sm hidden-xs"><%= song.author %></td>
    <td class="text-center">
      <% if song.published == true %>
        <span class="glyphicon glyphicon-ok text-success"></span>
      <% else %>
        <span class="glyphicon glyphicon-remove text-danger"></span>
      <% end %>
    </td>
  </tr>

<% end %>
