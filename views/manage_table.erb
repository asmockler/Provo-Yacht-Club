<% @posts.each do |post|  %>


  <tr id="<%= post["_id"] %>">
    <td><a class="edit btn btn-primary btn-xs btn-block" href="/Manager/edit-modal/<%= post['_id'] %>"> <span class="glyphicon glyphicon-pencil" data-toggle="tooltip" title="Edit"></span> </a></td>
    <td><a class="delete btn btn-danger btn-xs btn-block" href="/Manager/delete/<%= post['_id'] %>"><span class="glyphicon glyphicon-trash" data-toggle="tooltip" title="Delete"></span></a></td>
    <td><%= post['Song-Title'] %></td>
    <td class="hidden-sm hidden-xs"><%= post['Artist'] %></td>
    <td class="hidden-sm hidden-xs"><%= post['Album'] %></td>
    <td class="hidden-sm hidden-xs"><%= post['entrytime'] %></td>
    <td class="hidden-sm hidden-xs"><%= post['Author'] %></td>
    <td class="text-center">
      <% if post['publish'] == true %>
        <span class="glyphicon glyphicon-ok text-success"></span>
      <% else %>
        <span class="glyphicon glyphicon-remove text-danger"></span>
      <% end %>
    </td>
  </tr>

<%end%>
